{
  "experiment": {
    "scope": "demo-ecml-pkdd",
    "parameters": {
      "lock_release_tout": 120,
      "propagate": [
        { "in_sections": ["doe-triplets/explainer"], "params": {"fold_id": -1}},
        { "in_sections": ["doe-triplets/oracle"], "params": {"fold_id": -1, "retrain": true}}
      ],
      "expand": {"folds": ["doe-triplets/explainer"], "triplets": true}
    }
  },
  "doe-triplets": [
    {
      "dataset": {
        "class": "src.dataset.dataset_base.Dataset",
        "parameters": {
          "generator": {
            "class": "lab.src.dataset.trisqr_rand.TringlesSquares", 
            "parameters": { "num_instances": 128 }
          }
        } 
      },
      "oracle":{
        "class": "lab.src.oracle.oracle_trisqr.TrianglesSquaresOracle",
        "parameters": {}
      },
      "explainer": {   
        "class": "src.explainer.future.search.dces.DCESExplainer",
        "parameters": {}
      }
    }
  ],
  "evaluator": {
    "class": "src.evaluation.future.evaluator.Evaluator",
    "parameters": {
      "compose_pip": "./lab/config/snippets/default_pipeline.json"
    }
  },
  "compose_strs": "./lab/config/snippets/default_store_paths.json"
}